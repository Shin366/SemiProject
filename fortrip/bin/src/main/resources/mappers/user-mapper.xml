<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fortrip.com.domain.admin.user.model.mapper.UserMapper">
	<insert id="userAdd">
		INSERT INTO MEMBER VALUES (SEQ_MEMBER_NO.NEXTVAL,
		#{memberName}, #{nickName}, #{gender}, #{memberId}, #{memberPw}, #{phone}, #{email}, DEFAULT, #{birthDateYear},
		#{birthDateMon}, #{birthDateDay}, #{adminYn},DEFAULT)
	</insert>
</mapper>